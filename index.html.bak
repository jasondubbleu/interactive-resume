    <!-- Toggle Details Script -->
    <script>
      /**
       * Toggles the 'hidden' class on the element with the given ID.
       * Used to expand/collapse the details in the Experience section.
       *
       * @param {string} id - The ID of the element to show/hide.
       */
      function toggleDetails(id) {
        var elem = document.getElementById(id);
        if (elem.classList.contains('hidden')) {
          elem.classList.remove('hidden');
        } else {
          elem.classList.add('hidden');
        }
      }
    </script>
	
	<script>
  // Create a single tooltip element and add to the page
  const tooltip = document.createElement('div');
  tooltip.className = 'skill-tooltip';
  document.body.appendChild(tooltip);

  // For every bar image
  document.querySelectorAll('.skill-bar').forEach(bar => {
    // On mouse move over the bar, update text & position
    bar.addEventListener('mousemove', e => {
      const text = bar.dataset.proficiency;
      tooltip.textContent = `Proficiency: ${text}`;
      tooltip.style.left = e.clientX + 'px';
      tooltip.style.top  = e.clientY + 'px';
      tooltip.style.opacity = '1';
    });
    // On leaving the bar, hide the tooltip
    bar.addEventListener('mouseleave', () => {
      tooltip.style.opacity = '0';
    });
  });
</script>

<script>
  // Grab the button and the nav-dial
  const collapseBtn = document.querySelector('.collapse-btn');
  const navDial    = document.querySelector('.nav-dial');

  collapseBtn.addEventListener('click', () => {
    const isCollapsed = navDial.classList.toggle('collapsed');
    // Flip the arrow direction
    collapseBtn.textContent = isCollapsed ? '<<' : '>>';
    collapseBtn.setAttribute(
      'aria-label',
      isCollapsed ? 'Expand navigation dial' : 'Collapse navigation dial'
    );
  });
</script>

